# MARAG Configuration File
# Copy this file to .env and fill in your API keys

# Required API Keys
OPENAI_API_KEY=your_openai_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here  # For web search functionality

# Optional configurations
LANGCHAIN_API_KEY=your_langchain_api_key_here  # For LangSmith tracing
LANGCHAIN_PROJECT=MARAG  # For LangSmith project tracking

# Model configurations
OPENAI_MODEL=gpt-4o-mini  # You can change this to gpt-4, gpt-3.5-turbo, etc.

# Pathway configurations (for vector database)
PATHWAY_URL=http://172.30.2.194:8767  # Update this URL if needed